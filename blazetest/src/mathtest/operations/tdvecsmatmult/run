PATH_TDVECSMATMULT=$( dirname "${BASH_SOURCE[0]}" )

echo " Running dense vector/sparse matrix multiplication tests...";

EXE=$PATH_TDVECSMATMULT/V2aMCa; if [ -x $EXE ]; then $EXE; if [ $? != 0 ]; then exit 1; fi fi
EXE=$PATH_TDVECSMATMULT/V2aMCb; if [ -x $EXE ]; then $EXE; if [ $? != 0 ]; then exit 1; fi fi
EXE=$PATH_TDVECSMATMULT/V2bMCa; if [ -x $EXE ]; then $EXE; if [ $? != 0 ]; then exit 1; fi fi
