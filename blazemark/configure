// suite.
*/
EOF

if [ "$MTL" = "yes" ]; then
cat >> ./blazemark/system/MTL.h <<EOF
#define BLAZEMARK_MTL_MODE 1
EOF
else
cat >> ./blazemark/system/MTL.h <<EOF
#define BLAZEMARK_MTL_MODE 0
EOF
fi

cat >> ./blazemark/system/MTL.h <<EOF
//*************************************************************************************************

#endif
EOF


