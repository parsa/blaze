cat >> ./blazemark/system/BLAS.h <<EOF




//=================================================================================================
//
//  BLAS INCLUDE FILE CONFIGURATION
//
//=================================================================================================

#if BLAZEMARK_BLAS_MODE
extern "C" {
#include <$BLAS_INCLUDE_FILE>
}
#endif

#endif
EOF
else
cat >> ./blazemark/system/BLAS.h <<EOF

#endif
EOF
fi


