\$(BINARY_PATH)/custom: $CUSTOM
	@\$(CXX) \$(CXXFLAGS) -o \$(BINARY_PATH)/custom $CUSTOM \$(LIBRARIES)
	@echo "... finished"
	@echo
\$(OBJECT_PATH)/BLAZE_Custom.o:
	@echo
	@echo "Building custom expression (custom) binary..."
	@echo "  Building the Blaze kernel..."
	@\$(CXX) \$(CXXFLAGS) -c -o \$(OBJECT_PATH)/BLAZE_Custom.o \$(INSTALL_PATH)/src/blaze/Custom.cpp \$(INCLUDES)
\$(OBJECT_PATH)/BOOST_Custom.o:
	@echo "  Building the Boost uBLAS kernel..."
	@\$(CXX) \$(CXXFLAGS) -c -o \$(OBJECT_PATH)/BOOST_Custom.o \$(INSTALL_PATH)/src/boost/Custom.cpp \$(INCLUDES)
EOF

if [ "$BLITZ" = "yes" ]; then
cat >> Makefile <<EOF
\$(OBJECT_PATH)/BLITZ_Custom.o:
	@echo "  Building the Blitz++ kernel..."
	@\$(CXX) \$(CXXFLAGS) -c -o \$(OBJECT_PATH)/BLITZ_Custom.o \$(INSTALL_PATH)/src/blitz/Custom.cpp \$(INCLUDES)
EOF
fi

if [ "$GMM" = "yes" ]; then
cat >> Makefile <<EOF
\$(OBJECT_PATH)/GMM_Custom.o:
	@echo "  Building the GMM++ kernel..."
	@\$(CXX) \$(CXXFLAGS) -c -o \$(OBJECT_PATH)/GMM_Custom.o \$(INSTALL_PATH)/src/gmm/Custom.cpp \$(INCLUDES)
EOF
fi

if [ "$ARMADILLO" = "yes" ]; then
cat >> Makefile <<EOF
\$(OBJECT_PATH)/ARMADILLO_Custom.o:
	@echo "  Building the Armadillo kernel..."
	@\$(CXX) \$(CXXFLAGS) -c -o \$(OBJECT_PATH)/ARMADILLO_Custom.o \$(INSTALL_PATH)/src/armadillo/Custom.cpp \$(INCLUDES)
EOF
fi

if [ "$MTL" = "yes" ]; then
cat >> Makefile <<EOF
\$(OBJECT_PATH)/MTL_Custom.o:
	@echo "  Building the MTL kernel..."
	@\$(CXX) \$(CXXFLAGS) -c -o \$(OBJECT_PATH)/MTL_Custom.o \$(INSTALL_PATH)/src/mtl/Custom.cpp \$(INCLUDES)
EOF
fi

if [ "$EIGEN" = "yes" ]; then
cat >> Makefile <<EOF
\$(OBJECT_PATH)/EIGEN_Custom.o:
	@echo "  Building the Eigen kernel..."
	@\$(CXX) \$(CXXFLAGS) -c -o \$(OBJECT_PATH)/EIGEN_Custom.o \$(INSTALL_PATH)/src/eigen/Custom.cpp \$(INCLUDES)
EOF
fi

cat >> Makefile <<EOF
\$(OBJECT_PATH)/MAIN_Custom.o:
	@echo "  Building the benchmark..."
	@\$(CXX) \$(CXXFLAGS) -DINSTALL_PATH='"\$(INSTALL_PATH)"' -c -o \$(OBJECT_PATH)/MAIN_Custom.o \$(INSTALL_PATH)/src/main/Custom.cpp \$(INCLUDES)


# Clean up rules
clean:
	@echo "Cleaning up..."
	@\$(MAKE) --no-print-directory -C \$(INSTALL_PATH)/src/clike clean
	@\$(MAKE) --no-print-directory -C \$(INSTALL_PATH)/src/classic clean
	@\$(MAKE) --no-print-directory -C \$(INSTALL_PATH)/src/blas clean
	@\$(MAKE) --no-print-directory -C \$(INSTALL_PATH)/src/blaze clean
	@\$(MAKE) --no-print-directory -C \$(INSTALL_PATH)/src/boost clean
	@\$(MAKE) --no-print-directory -C \$(INSTALL_PATH)/src/blitz clean
	@\$(MAKE) --no-print-directory -C \$(INSTALL_PATH)/src/gmm clean
	@\$(MAKE) --no-print-directory -C \$(INSTALL_PATH)/src/armadillo clean
	@\$(MAKE) --no-print-directory -C \$(INSTALL_PATH)/src/flens clean
	@\$(MAKE) --no-print-directory -C \$(INSTALL_PATH)/src/mtl clean
	@\$(MAKE) --no-print-directory -C \$(INSTALL_PATH)/src/eigen clean
	@\$(MAKE) --no-print-directory -C \$(INSTALL_PATH)/src/main clean
	@\$(RM) bin/* obj/*.o


# Setting the independent commands
.PHONY: default clean \\
